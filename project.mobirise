                }
              }
            },
            ".team-card": {
              "margin-bottom": "2rem",
              "transition": "all 0.3s",
              "&:hover": {
                "transform": "translateY(-10px)"
              }
            },
            ".card-wrap": {
              "background": "@cardBg",
              "border": "1px solid rgba(0, 0, 0, 0.1)",
              ".image-wrap img": {
                "width": "100%"
              },
              ".content-wrap": {
                "padding": "1.5rem",
                ".mbr-section-btn": {
                  "padding-top": "1rem",
